{
  "name": "adon-api",
  "version": "2.0.1",
  "description": "API for all Adon-based projects",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "scripts": {
    "start": "node dist/src/sample.js",
    "start:dev": "ts-node sample",
    "dist:server": "node dist/src/sample.js",
    "start:server": "ts-node sample",
    "audit": "npm audit",
    "lint": "eslint \"src/**/*.ts\" \"sample/**/*.ts\"",
    "snyk": "snyk test",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisv79/ADON-API.git"
  },
  "keywords": [
    "adon",
    "api"
  ],
  "author": "Adonis Lee Villamor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adonisv79/ADON-API/issues"
  },
  "homepage": "https://github.com/adonisv79/ADON-API#readme",
  "dependencies": {
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "helmet": "4.4.1",
    "http-error-types": "1.0.1",
    "ioredis": "4.26.0",
    "logger-template": "1.0.1",
    "rate-limiter-flexible": "2.2.1",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/cors": "2.8.10",
    "@types/express": "4.17.11",
    "@types/helmet": "4.0.0",
    "@types/ioredis": "4.22.3",
    "@types/node": "14.14.37",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "eslint": "7.24.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-plugin-import": "2.22.1",
    "typescript": "4.2.4"
  }
}
